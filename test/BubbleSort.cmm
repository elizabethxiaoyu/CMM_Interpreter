//定义数组
int[6] arr;
arr[0] = 6;
arr[1] = 3;
arr[2] = 8;
arr[3] = 2;
arr[4] = 9;
arr[5] = 1;
//打印原数组值ֵ
write("原数组：");
for(int i = 0; i < 6; i = i + 1){
    write(arr[i]);
}
//冒泡排序 
for(i = 0; i < 5; i = i + 1) {
    for(int j = 0; j < 5; j = j + 1) {
            if(arr[j] > arr[j+1]) {
                        int temp = arr[j];
			arr[j] = arr[j+1];
			arr[j+1] = temp;
	    } 
    } 
}
//打印排序后数组值
write("排序后数组：");
for(i = 0; i < 6; i = i + 1) {
    write(arr[i]); 
}